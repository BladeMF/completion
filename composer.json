{
    "name": "phpactor/completion",
    "minimum-stability": "dev",
    "description": "Completion library for Worse Reflection",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "phpactor/worse-reflection": "~0.2",
        "phpactor/source-code-filesystem": "dev-master",
        "phpactor/class-to-file": "dev-master",
        "phpactor/text-document": "^1.0@dev"
    },
    "require-dev": {
        "phpbench/phpbench": "^1.0@dev",
        "phpunit/phpunit": "^7.0",
        "phpactor/test-utils": "^1.0@dev",
        "phpstan/phpstan": "^0.10.0@dev",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Completion\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Completion\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
